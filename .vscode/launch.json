{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "attach",
			"name": "Docker: Attach to Node",
			"remoteRoot": "/usr/src/app"
		},
		{
			"name": "backend1",
			"type": "go",
			"request": "attach",
			"mode": "remote",
			"port": 8080,
			"host": "127.0.0.1",
			"remotePath": "go/src/",
			"showLog": true,
			"cwd": "${workspaceFolder}/backend/"
		}
	]
}
